{
    "window.titleBarStyle": "custom",
    "workbench.colorTheme": "GitHub Light",
    "editor.tabSize": 2,
    "editor.fontFamily": "'FiraCode Nerd Font', 'monospace', monospace",
    "todo-tree.regex.regex": "(|#|<!--|;|/\\*|^|^[ \\t]*(-|\\d+.))\\s*($TAGS)",
    "git.confirmSync": false,
    "solidity-va.codelens.drawio.enable": false,
    "solidity-va.outline.decorations": false,
    "solidity-va.deco.statevars": false,
    "rust-analyzer.inlayHints.maxLength": 10,
    "rust-analyzer.rustfmt.extraArgs": ["+nightly"],
    "rust-analyzer.check.overrideCommand": [
        "cargo",
        "+nightly",
        "clippy",
        "--all",
        "--all-features",
        "--message-format=json"
      ],

    "[rust]" : {
        "editor.tabSize": 4,
        "editor.formatOnSave": true
    },
    "[huff]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "[noir]": {
        "editor.tabSize": 4,
        "editor.insertSpaces": true
    },
    "explorer.confirmDragAndDrop": false,
    "redhat.telemetry.enabled": false,
    "diffEditor.codeLens": true,
    "todo-tree.highlights.useColourScheme": true,
    "python.analysis.extraPaths": [
        "./scripts",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.6.5-linux-x64"
    ],
    "python.autoComplete.extraPaths": [
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.0.413-linux-x64",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.1.40-linux-x64",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.1.41-linux-x64",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.2.0-linux-x64",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.5.0-linux-x64",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.6.0-linux-x64",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.6.3-linux-x64",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.6.4-linux-x64",
        "/home/tyler/.vscode-oss/extensions/continue.continue-0.6.5-linux-x64"
    ],
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "workbench.sideBar.location": "right"
}
